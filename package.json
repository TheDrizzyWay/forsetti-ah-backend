{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "pretest": "cross-env NODE_ENV=test npm run migrate:undo && cross-env NODE_ENV=test npm run migrate && cross-env NODE_ENV=test npm run seed",
    "test": "cross-env NODE_ENV=test nyc mocha -c --exit --require @babel/register --timeout 600000 ./server/test/**/*.spec.js",
    "start": "node build/index.js",
    "build": "npm run remove:build && babel index.js -d build && babel server -d build/server",
    "remove:build": "rm -rf ./build",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node index.js",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "drop": "node_modules/.bin/sequelize db:drop",
    "create": "node_modules/.bin/sequelize db:create",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "heroku:migrate": "npm run migrate:undo && npm run migrate && npm run seed",
    "heroku-postbuild": "npm run build && npm run heroku:migrate"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.17.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "editorconfig": "^0.15.3",
    "express": "^4.18.2",
    "express-session": "^1.17.0",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.3.0",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^2.2.1",
    "nodemailer": "^6.6.1",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "reading-time": "^1.2.0",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^5.5.1",
    "slug": "^0.9.1",
    "slugify": "^1.3.6",
    "string-strip-html": "^4.3.7",
    "swagger-ui-express": "^4.6.0",
    "validator": "^13.7.0",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0",
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "@babel/runtime": "^7.7.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^5.2.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.2",
    "nock": "^10.0.6",
    "nodemon": "^2.0.20",
    "nyc": "^13.3.0"
  }
}
